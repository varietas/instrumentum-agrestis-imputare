<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder class="ch.qos.logback.classic.encoder.PatternLayoutEncoder">
            <Pattern>
                %d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - %msg%n
            </Pattern>
        </encoder>
    </appender>
    
    <root level="INFO" />
    <logger name="io.varietas.agrestis.imputare.searching" level="TRACE">
        <appender-ref ref="STDOUT" />
    </logger>
    <logger name="io.varietas.agrestis.imputare.utils" level="DEBUG">
        <appender-ref ref="STDOUT" />
    </logger>
    <!--    <logger name="de.freecossack.core.configuration" level="TRACE">
        <appender-ref ref="STDOUT" />
    </logger>-->
</configuration>